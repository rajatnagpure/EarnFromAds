<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajatnagpure.earnfromads">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/earn_rupees_logo_square"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/earn_rupees_logo_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.EarnFromAds">
        <activity
            android:name=".banktransfer.BankTransferList"
            android:exported="false" />
        <activity android:name=".earn.NotLoading" />
        <activity android:name=".googleplay.GooglePlayRedeemList" />
        <activity android:name=".earn.Earn" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>